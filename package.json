{
  "name": "kokkok-messenger-desktop",
  "version": "0.1.0",
  "main": "electron/main.js",
  "homepage": "./",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "postinstall": "npx husky install",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "npm-run-all --parallel start-web start-electron",
    "start-win": "npm-run-all --parallel start-web-win start-electron-win",
    "start-web": "export BROWSER=none && react-scripts start",
    "start-web-win": "set BROWSER=none && react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "start-electron-win": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build": "react-scripts build",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron",
    "build-electron-win": "mkdir build\\src && Xcopy /E /I /Y electron build\\electron",
    "package": "npm run build && npm run build-electron && electron-builder build -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-win": "npm run build && npm run build-electron-win && electron-builder build -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.10.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@sentry/electron": "^3.0.7",
    "@sentry/react": "^7.8.0",
    "@sentry/tracing": "^7.8.0",
    "@types/react-redux": "^7.1.23",
    "amazon-chime-sdk-js": "^3.6.0",
    "axios": "^0.26.1",
    "buffer": "^6.0.3",
    "cropperjs": "^1.5.12",
    "encode": "^1.0.1",
    "faker": "^5.1.0",
    "i18next": "^21.8.14",
    "is-electron": "^2.2.1",
    "jotai": "^1.7.3",
    "libphonenumber-js": "^1.9.51",
    "luxon": "^3.0.1",
    "moment": "^2.29.3",
    "node-machine-id": "^1.1.12",
    "query-string": "^7.1.1",
    "random-words": "^1.1.2",
    "react": "^17.0.2",
    "react-aws-s3": "^1.5.0",
    "react-canvas-video": "^0.0.4",
    "react-cookie": "^4.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-cropper": "^2.1.8",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-easy-crop": "^4.2.0",
    "react-giphy-searchbox": "^1.5.4",
    "react-giphy-searchbox-stickers": "^1.0.5",
    "react-highlight-words": "^0.18.0",
    "react-html5-video-editor": "^1.2.0",
    "react-i18next": "^11.18.1",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-inlinesvg": "^2.3.0",
    "react-menu-list": "^7.0.3",
    "react-mic": "^12.4.6",
    "react-qr-code": "^2.0.7",
    "react-query": "^3.34.19",
    "react-redux": "^7.2.8",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "react-scroll-horizontal": "^1.6.6",
    "react-scrollable-picker": "^1.0.2",
    "react-slick": "^0.29.0",
    "react-toastify": "^8.2.0",
    "react-window-opener": "^0.0.7",
    "redux-saga": "^1.1.3",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.5.1",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.3.4",
    "swr": "^1.3.0",
    "typesafe-actions": "^5.1.0",
