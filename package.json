  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^3.0.0",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "autoprefixer": "^10.4.4",
    "electron": "^18.2.0",
    "electron-builder": "^22.14.13",
    "eslint": "^7.14.0",
    "eslint-config-react-app": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.kokkokmessenger",
    "productName": "kokkok-messenger-desktop",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  }
}
